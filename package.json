{
  "name": "byticular",
  "version": "1.0.0",
  "description": "A platform for better coding tests.",
  "main": "index.html",
  "scripts": {
    "devserver": "live-server",
    "watch:sass": "sass src/sass/main.scss src/css/style.css -w",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass src/sass/style.scss src/css/style.comp.css",
    "prefix:css": "postcss use --use autoprefixer -b 'last 10 versions' src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "node-sass src/css/style.prefix.css src/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [
    "coding",
    "tests",
    "modern",
    "cms"
  ],
  "author": "Blessed Zulu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.12"
  }
}